{
  "name": "Objects remover",
  "description": "Remove selected objects",
  "homePage": "index",
  "ajaxServer": "ajax.js",
  "execMethod": "post",
  "launcher": "nodeModule",
    "startAjaxAsThread": true,
    "launcherPrms": {
        "javaScript": "server.js",
        "startAsThread": true
    },
  "staticDir": "static",
  "callbackBeforeExec": "callbackBeforeExec",
  "onChangeObjectMenuEvent": "callback:onChangeObjects",
  "timeout": 180,
  "showWhenNoObjectsSelected": false,
  "outputFormat": "text",
  "descriptionTemplate": "Removing objects: %:o:%.",
  "icon": "delete",
  "parameters": {
    "o": {
      "description": "These objects will be removed from the database"
    }
  }
}
