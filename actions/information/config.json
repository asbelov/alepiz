{
    "name": "Objects information",
    "description": "Show objects properties and history values",
    "homePage": "index",
    "ajaxServer": "ajax.js",
    "startAjaxAsThread": true,
    "staticDir": "static",
    "showWhenNoObjectsSelected": true,
    "onChangeObjectMenuEvent": "callback:onChangeObjects",
    "icon": "settings",
	"CSVDiv": ";",
	"CSVDecimalSep": ",",
  	"properties": {
    	"Service name": {
        	"property": "SERVICE_NAME"
        },
        "Server": {
        	"property": "ZABBIX_HOSTNAME"
        },
      	"Executable": {
        	"property": "SERVICE_PATH",
          	"replaceRE": {
                "regExp": "^.+?\\\\([^\\\\]+?\\.exe).*?$",
              	"replaceTo": "$1",
              	"flags": "i"
        	}
        },
      	"Service startup": {
        	"property": "SERVICE_STARTUP",
          	"valueMap": {
            	"0": "Automatic",
				"1": "Automatic delayed",
				"2": "Manual",
				"3": "Disabled",
				"4": "Unknown"
            }
        },
      	"Service state": {
          	"counter": "I: Service state",
			"axisY": "left",
          	"history_function": "last()",
          	"valueMap": {
            	"0": "Running",
				"1": "Paused",
				"2": "Start pending",
				"3": "Pause pending",
				"4": "Continue pending",
				"5": "Stop pending",
				"6": "Stopped",
				"7": "Unknown",
				"255": "No such service"
            }
    	},
		"CPU cores": {
			"counter": "I: CPUs number"
		},
		"Total memory": {
			"counter": "I: Total memory",
			"toHuman": "Bytes"
		},
		"Free memory threshold": {
			"property": "FREE_MEMORY_LIMIT"
		},
		"Free memory (min in 10m)": {
			"counter": "I: Free memory",
			"axisY": "right",
			"history_function": "min(10m)",
			"toHuman": "Bytes"
		},
		"Idle disk time (min in 10m)": {
			"counter": "I: % Idle Disk Time",
			"axisY": "left",
			"history_function": "min(10m)",
			"toHuman": "Percents"
		},
		"Uptime": {
			"counter": "I: Host uptime",
			"multiplier": 1000,
			"toHuman": "Time"
		},
		"IP address": {
			"property": "HOST"
		},
		"Ping (avg in 10m)": {
			"counter": "I: Ping host every 30 sec",
			"axisY": "left",
			"history_function": "avg(10m)",
			"replaceRE": {
				"regExp": "^(\\d+\\.\\d\\d?).*$",
				"replaceTo": "$1ms"
			}
		},
		"CPU Time (max in 10m)": {
			"counter": "I: % CPU Time avg 5 min",
			"axisY": "left",
			"history_function": "max(10m)",
			"toHuman": "Percents"
		},
		"Volume name": {
			"property": "VOLUME_NAME"
		},
		"Total disk space": {
			"counter": "I: Total disk space",
			"toHuman": "Bytes"
		},
		"Free disk space (min in 1h)": {
			"counter": "I: Free disk space",
			"axisY": "left",
			"history_function": "min(1h)",
			"toHuman": "Bytes"
		},
		"Network interface name": {
			"property": "IFNAME"
		},
		"Current Bandwidth": {
			"counter": "I: Current Bandwidth",
			"toHuman": "Bits"
		},
		"Bytes Received/sec (max in 10m)": {
			"counter": "I: Bytes Received/sec",
			"axisY": "left",
			"history_function": "max(10m)",
			"toHuman": "Bytes/sec"
		},
		"Bytes Sent/sec (max in 10m)": {
			"counter": "I: Bytes Sent/sec",
			"axisY": "left",
			"history_function": "max(10m)",
			"toHuman": "Bytes/sec"
		},
		"Packets Received/sec (max in 10m)": {
			"counter": "I: Packets Received/sec",
			"axisY": "right",
			"history_function": "max(10m)"
		},
		"Packets Sent/sec (max in 10m)": {
			"counter": "I: Packets Sent/sec",
			"axisY": "right",
			"history_function": "max(10m)"
		},
		"Output Queue Length (max in 10m)": {
			"counter": "I: Output Queue Length",
			"history_function": "max(10m)"
		}
	}
}